                  </VBox>
                    </children>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
                </HBox>
                <HBox alignment="BOTTOM_CENTER" spacing="5.0">
                    <children>
                  <VBox prefHeight="42.0" prefWidth="291.0">
                     <children>
                        <Label text="%cidadesing.text" />
                        <ComboBox fx:id="cmbBxCidade" prefHeight="25.0" prefWidth="296.0" />
                     </children>
                  </VBox>
                        <Button fx:id="btnConfirma" mnemonicParsing="false" onAction="#btnConfirmaClick" prefWidth="100.0" text="%confirmar.text">
                        </Button>
                        <Button mnemonicParsing="false" onAction="#btnCancelaClick" prefWidth="100.0" text="%cancelar.text">
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="5.0" top="2.0" />
                    </padding>
                </HBox>
            
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
    </children>
